pragma solidity ^0.8.0;
contract SimpleMath {
// Initialization of num1 and num2
uint256 num1 = 10;
uint256 num2 = 5;
uint public additionResult = num1 + num2; // addition
uint public subtractionResult = num1 - num2; // subtraction
uint public multiplicationResult = num1 * num2; // multiplication
uint public divisionResult = num1 / num2; // division
}